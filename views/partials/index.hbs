



    <!-- Main jumbotron for a primary marketing message or call to action -->
    <div class="jumbotron">
        <div class="container">
            <h1>{{ title }}</h1>
            <p>{{ splashText }}</p>
        </div>
    </div>

    <div class="container">
        <!-- Example row of columns -->
        <div class="row">

            {{#each paragraphs }}

            <div class="col-md-6">
                <h2>{{ header }}</h2>
                <p>{{{ text }}}</p>
                <p><a class="{{ link.className }}" href="{{ link.href }}" role="button">{{ link.text }}</a></p>
            </div>


            {{/each}}



        </div>
